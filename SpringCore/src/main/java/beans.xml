<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="helloWorld" class="com.bridgelabz.SpringCore.HelloWorld">
		<property name="message" value="Hello World!" />
	</bean>
	<bean name="student" class="com.bridgelabz.SpringCore.Student">
		<property name="name" value="vikash"></property>
		<property name="email" value="vikash@gmail.com"></property>
		<property name="cls" value="vikash"></property>

	</bean>
</beans>
